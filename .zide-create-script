typeset -g zide_ef
zide_ef=( ${(k)functions} )
trap "unset -f -- \"\${(k)functions[@]:|zide_ef}\" &>/dev/null; unset zide_ef" EXIT
trap "unset -f -- \"\${(k)functions[@]:|zide_ef}\" &>/dev/null; unset zide_ef; return 1" INT

local name="$1"
local -a mods
mods=( ${@[2,-1]} )

print -rl $mods

# vim:ft=zsh:sw=4:sts=4:noet:
