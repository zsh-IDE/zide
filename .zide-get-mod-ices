local __var_name=$1

local -A __Mod_Ices

__Mod_Ices=(
    # Zdharma
    zshelldoc       "make'install PREFIX=bin'"
    zflai           ""
    zui             ""
    zsh-string-lib  ""
    # Zsh-ide
    shu2            ""
    # Molovo
    zunit           "atclone'./build' atpull'%atclone'"
    revolver        "atclone'./build' atpull'%atclone'"
    # Jsks
    czhttpd         "jsks/czhttpd"
    # Mvdan
    shfmt           "from'gh-r'"
)

: ${(@PAA)__var_name::="${(kv)__Mod_Ices[@]}"}

